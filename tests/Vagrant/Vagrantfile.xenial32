unless Vagrant::DEFAULT_SERVER_URL.frozen?
  Vagrant::DEFAULT_SERVER_URL.replace('https://vagrantcloud.com')
end

Vagrant.configure(2) do |config|
  config.vm.box = "ubuntu/xenial32"

  config.vm.provider "virtualbox" do |vb|
    vb.memory = "512"
  end
end

